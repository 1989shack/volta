{
  "version": "0.2.0",
  "configurations":[
    {
      "type": "lldb",
      "request": "launch",
      "name": "Cargo run notion core",
      "cargo": {
        "args": [
          "run",
          "--bin", "notion",
        ],
        "filter": {
          "kind": "bin",
          "name": "notion"
        }
      },
      "program": "${cargo:program}",
      "args": ["fetch", "2bar"],
      "sourceLanguages": ["rust"]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Cargo test notion core",
      "cargo": {
        "args": [
          "test",
          "--lib",
          "--no-run",
          "--package", "notion-core",
          "--", "--test-threads", "1",
        ],
        "filter": {
          "kind": "lib",
          "name": "notion-core"
        }
      },
      "program": "${cargo:program}",
      "args": [],
      "sourceLanguages": ["rust"]
    }
  ]
}